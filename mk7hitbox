_0x1D.CornerRadius = UDim.new(0, 8)
_0x1D.Parent = _0x1C

-- Ação do botão
_0x19.MouseButton1Click:Connect(function()
    if not _0x14() then return end -- Verifica exploit novamente
    local _0x1E = tonumber(_0x1C.Text)
    if _0x1E and _0x1E > 0 and _0x1E <= _0x5 then
        _0x13(_0x1E)
        print("Hitboxes ajustadas para: " .. _0x1E)
    else
        warn("Erro: Insira um número válido (1 a " .. _0x5 .. ").")
    end
end)

-- Aplica hitboxes para novos jogadores
_0x1.PlayerAdded:Connect(function(_0x1F)
    if _0x1F ~= _0x2 then
        _0x1F.CharacterAdded:Connect(function()
            _0x9(_0x1F, tonumber(_0x1C.Text) or _0x4)
        end)
    end
end)

-- Aplica hitboxes aos jogadores existentes
for _, _0x1F in pairs(_0x1:GetPlayers()) do
    if _0x1F ~= _0x2 and _0x1F.Character then
        _0x9(_0x1F, _0x4)
    end
end

-- Atualiza hitboxes ao sair
_0x1.PlayerRemoving:Connect(function(_0x20)
    if _0x20 ~= _0x2 then
        _0x13(tonumber(_0x1C.Text) or _0x4)
    end
end)

-- Depuração
print(_0x3 .. " inicializada.")
print("Frame: " .. (_0x17.Name or "Sem nome"))
print("Botão: " .. _0x19.Name)
print("TextBox: " .. _0x1C.Name)
